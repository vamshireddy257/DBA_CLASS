set lines 200
set pages 20
col pdb for a20
col NETWORK_NAME for a30

SELECT PDB, NETWORK_NAME, CON_ID
FROM CDB_SERVICES
WHERE PDB IS NOT NULL
AND CON_ID > 2
ORDER BY PDB;

